                        </resource-match>
                        <resource-match attr="resource-id">
                            PIM.onAddressBookItemFound
                        </resource-match>

                        <!-- This part contains api-features defined in WAC1.0 -->

                        <resource-match attr="resource-id">
                            http://jil.org/jil/api/1.1/accelerometerinfo
